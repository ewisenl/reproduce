# Ignore generated files
web/sites/*/files/css
web/sites/*/files/js
web/sites/*/files/php
web/sites/*/files/languages
web/sites/*/files/translations

# Ignore credentials
auth.json
auth.env

# Ignore directories generated by Composer
drush/contrib
vendor
web/core
web/modules/contrib
web/themes/contrib
web/profiles/contrib

# Ignore Drupal's file directory
## web/sites/*/files

# Ignore files generated by PhpStorm
.idea

# Ignore files generated by MacOS
.DS_Store

# Ignore configuration files that may contain sensitive information.
web/sites/*/settings.local.php

# Ignore archives
*.sql
*.gz
*.tar
*.zip

# Explicitly allow default.sql.gz
!default.sql.gz

# Lint cache
.phplint-cache
